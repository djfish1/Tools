"colorscheme default
"hi DiffText term=reverse cterm=bold ctermbg=12 gui=none guibg=LightGreen

colorscheme elflord

set guifont=Monospace\ 10

command! FontNormal set guifont=Monospace\ 10
command! FontBig set guifont=Monospace\ 12
command! FontHuge set guifont=Monospace\ 16

set guioptions-=T

set nocursorline
set nocursorcolumn
